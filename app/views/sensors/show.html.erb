<div class="sensors">
  <div class="container">
      <h1>Readings</h1>

    <h2><%= @sensor.name %></h2>

    <div class="cards row">
      <% @readings.each do |r| %>
      <div class="card col-xs-10">
        <p><%= r.valor %></p>
		<p class="time"><%= r.created_at %></p><BR>
      </div>
      <% end %>
    </div>
    <%= link_to 'Back', :action => "home" %>
  </div>
</div>