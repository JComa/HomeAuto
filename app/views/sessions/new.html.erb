<div class="create">
	<div class="container">
		<div class="form">

      <h1>Log in</h1>
			
			<%= form_for(:session, url: login_path) do |f| %> 
			  <%= f.text_field :device_name, :placeholder => "Device name" %>
			  <%= f.password_field :password, :placeholder => "Password" %> 
			  <div class="actions">
			  <%= f.submit "Log in"%>
			  </div>
			<% end %>
		
    </div>
	</div>
</div>